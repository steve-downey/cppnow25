format:
  line_width: 119
  tab_size: 2
  max_subgroups_hwrap: 4
  max_rows_cmdline: 8
  separate_ctrl_name_with_space: false
  separate_fn_name_with_space: false
  dangle_parens: true
  dangle_align: prefix
  line_ending: unix
  keyword_case: upper
  always_wrap:
  - file
  - install
  - write_basic_package_version_file

markup:
  enable_markup: false
